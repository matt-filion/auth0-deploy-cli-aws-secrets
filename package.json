  "bugs": {
    "url": "https://github.com/matt-filion/auth0-deploy-cli-config-values/issues"
  },
  "keywords": [
    "auth0",
    "auth0-cli-deploy",
    "secrets",
    "AWS Secret Manager"
  ],
  "homepage": "https://github.com/matt-filion/auth0-deploy-cli-config-values#readme",
  "devDependencies": {
    "@types/node": "^10.12.12",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "aws-sdk": "^2.371.0",
    "commander": "^2.19.0"
  }
