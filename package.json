  },
  "files": [
    "dist/**/*",
    "bin/index"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matt-filion/auth0-deploy-cli-config-values.git"
  },
  "author": "matt.filion@gmail.com",
